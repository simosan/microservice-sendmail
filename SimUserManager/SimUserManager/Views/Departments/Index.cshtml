@model IEnumerable<SimUserManager.Models.Departments>

@{
    ViewData["Title"] = "部署情報";
}

<h1>部署情報</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Department)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Section)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Department)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Section)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.DepartmentNo">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.DepartmentNo">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.DepartmentNo">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
